    "import glob\n",
    "\n",
    "def builddf(htmlfiles):\n",
    "    df= pd.DataFrame()\n",
    "    for html in htmlfiles:\n",
    "        df1=parseRtergHtml(html)\n",
    "        if len(df) == 0:\n",
    "            df=df1\n",
    "        else:\n",
    "            df=df.append(df1, ignore_index = True)               \n",
    "    return df\n",
    "\n",
    "htmlfiles=glob.glob('rterg_html_outs/*.html')\n",
    "df = builddf(htmlfiles)\n",
    "print(df.head())\n"
   ]
